{
  "name": "theme",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel watch:js watch:sass",
    "watch:js": "babel-node tools/startBuild.js",
    "watch:theme": "browser-sync start",
    "watch:sass": "babel-node tools/watchSass",
    "lint": "eslint ./src/js/** ./tests/**",
    "test": "jest --watch"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "node_modules/jest-enzyme/lib/index.js",
    "testPathDirs": [
      "src/js/react"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "^HeaderConfigApp(.*)$": "<rootDir>/src/js/react/HeaderConfigApp$1",
      "^HeaderApp(.*)$": "<rootDir>/src/js/react/HeaderApp$1",
      "\\.(css|scss|less)$": "<rootDir>/tests/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/tests/__mocks__/fileMock.js"
    },
    "collectCoverage": true,
    "coverageReporters": [
      "text",
      "html"
    ]
  },
  "author": "",
  "license": "ISC",
