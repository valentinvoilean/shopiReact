{
  "name": "theme",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "Valentin Voilean <vali.voilean@gmail.com>",
  "license": "ISC",
  "repository": "git@bitbucket:vvm88/shopireact.git",
  "scripts": {
    "start": "npm run dev",
    "lint": "eslint --fix --ext .jsx,.js ./src",
    "test": "jest",
    "dev": "webpack-dev-server --config ./tools/webpack/webpack.config.dev.js",
    "build": "npm-run-all --parallel watch watch:js",
    "watch": "node tools/watch",
    "watch:js": "webpack --watch --config ./tools/webpack/webpack.config.prod.js"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/node_modules/jest-enzyme/lib/index.js",
    "roots": [
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|less)$": "<rootDir>/src/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/src/__mocks__/fileMock.js",
      "^styles(.*)$": "<rootDir>/src/static/styles$1",
      "^svg(.*)$": "<rootDir>/src/static/svg$1",
      "^modernizr$": "<rootDir>/.modernizrrc"
    },
    "collectCoverage": true,
    "coverageReporters": [
      "text",
      "html"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false,
          "useBuiltIns": false,
          "debug": false,
          "targets": {
            "browsers": [
              ">1%",
              "last 4 versions",
              "Firefox ESR",
              "not ie < 9"
            ]
          }
        }
      ],
      "stage-0",
      "react",
      "react-optimize"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ],
    "env": {
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      },
      "development": {
        "plugins": [
          "transform-react-jsx-self",
          "transform-react-jsx-source"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "react",
      "jsx-a11y",
      "jasmine"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "no-console": [
        "warn",
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ],
      "no-alert": [
        "error"
      ],
      "no-dupe-args": "error",
      "no-dupe-keys": "error",
      "no-duplicate-case": "error",
      "no-empty": "error",
      "no-ex-assign": "error",
      "no-extra-boolean-cast": "error",
      "block-scoped-var": "error",
      "curly": "error",
      "default-case": "error",
      "dot-location": [
        "error",
        "property"
      ],
      "eqeqeq": [
        "error",
        "always"
      ],
      "no-extend-native": "error",
      "no-extra-bind": "error",
      "no-extra-label": "error",
      "no-fallthrough": "error",
      "no-labels": "error",
      "no-lone-blocks": "error",
      "no-multi-spaces": "error",
      "no-redeclare": "error",
      "no-self-assign": "error",
      "no-self-compare": "error",
      "no-useless-call": "error",
      "no-undefined": "error",
      "no-unused-vars": "error",
      "no-use-before-define": "error",
      "camelcase": "error",
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "comma-spacing": [
        "error",
        {
          "before": false,
          "after": true
        }
      ],
      "comma-style": [
        "error",
        "last"
      ],
      "computed-property-spacing": [
        "error",
        "never"
      ],
      "eol-last": "error",
      "keyword-spacing": [
        "error",
        {
          "before": true
        }
      ],
      "max-statements-per-line": [
        "error",
        {
          "max": 2
        }
      ],
      "no-multiple-empty-lines": [
        "error",
        {
          "max": 2
        }
      ],
      "no-trailing-spaces": "warn",
      "quotes": [
        "error",
        "single",
        {
          "avoidEscape": true,
          "allowTemplateLiterals": true
        }
      ],
      "arrow-spacing": "error",
      "no-spaced-func": "error",
      "no-whitespace-before-property": "error",
      "space-in-parens": [
        "error",
        "never"
      ],
      "space-before-blocks": "error",
      "space-infix-ops": [
        "error",
        {
          "int32Hint": false
        }
      ],
      "no-const-assign": "error",
      "no-duplicate-imports": "error",
      "no-useless-constructor": "error",
      "no-var": "error",
      "semi": [
        "error",
        "always"
      ],
      "react/forbid-prop-types": [
        "error",
        {
          "forbid": [
            "any"
          ]
        }
      ],
      "react/no-children-prop": "error",
      "react/no-danger": "error",
      "react/no-did-mount-set-state": "error",
      "react/no-did-update-set-state": "error",
      "react/no-multi-comp": "error",
      "react/no-unescaped-entities": "error",
      "react/no-unused-prop-types": "error",
      "react/prefer-es6-class": "error",
      "react/require-default-props": "error",
      "react/require-optimization": "error",
      "react/self-closing-comp": [
        "error",
        {
          "component": true,
          "html": false
        }
      ],
      "react/sort-comp": "error",
      "react/style-prop-object": "error",
      "react/no-string-refs": "error",
      "jsx-quotes": [
        "error",
        "prefer-double"
      ],
      "react/jsx-boolean-value": [
        "error",
        "never"
      ],
      "react/jsx-closing-bracket-location": "error",
      "react/jsx-curly-spacing": "error",
      "react/jsx-equals-spacing": "error",
      "react/jsx-filename-extension": [
        "error",
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/jsx-key": "error",
      "react/jsx-no-comment-textnodes": "error",
      "react/jsx-no-target-blank": "error",
      "react/jsx-pascal-case": "error",
      "react/jsx-space-before-closing": "error",
      "react/jsx-tag-spacing": "error",
      "react/jsx-wrap-multilines": "error",
      "jsx-a11y/img-has-alt": "error",
      "jsx-a11y/img-redundant-alt": [
        "error",
        {
          "words": [
            "image",
            "photo",
            "picture"
          ],
          "components": [
            "Image"
          ]
        }
      ],
      "jsx-a11y/no-access-key": "error"
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:jsx-a11y/recommended",
      "plugin:jasmine/recommended"
    ],
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jquery": true,
      "jasmine": true,
      "jest": true
    }
  },
  "devDependencies": {
    "@shopify/themekit": "^0.6.6",
    "autoprefixer": "^6.7.6",
    "autoprefixer-loader": "3.2.0",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-plugin-transform-react-jsx-self": "^6.22.0",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.22.0",
    "bootstrap": "3.3.7",
    "chokidar": "1.6.1",
    "css-loader": "^0.27.1",
    "enzyme": "2.7.1",
    "eslint": "^3.17.1",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-jasmine": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "glob": "7.1.1",
    "html-loader": "^0.4.5",
    "jest": "^19.0.2",
    "jest-cli": "^19.0.2",
    "jest-enzyme": "^2.1.2",
    "jquery.currencies.js": "github:carolineschnapp/currencies",
    "json-loader": "0.5.4",
    "less": "2.7.2",
    "less-loader": "^3.0.0",
    "md5": "2.2.1",
    "node-normalize-scss": "1.4.0",
    "node-sass": "4.5.0",
    "node-watch": "0.4.1",
    "npm-run-all": "^4.0.2",
    "picturefill": "3.0.2",
    "postcss-loader": "^1.3.3",
    "progress-bar-webpack-plugin": "^1.9.3",
    "react-addons-test-utils": "15.4.2",
    "react-test-renderer": "15.4.2",
    "resolve-url": "0.2.1",
    "resolve-url-loader": "^2.0.2",
    "run-sequence": "1.2.2",
    "sass-loader": "^6.0.3",
    "sass-module-importer": "1.4.0",
    "serial-concat-files": "1.0.0",
    "style-loader": "^0.13.2",
    "svg-sprite-loader": "^0.3.0",
    "url-loader": "^0.5.8",
    "webpack": "2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-merge": "^4.0.0",
    "webpack-modernizr-loader": "^3.0.0"
  },
  "dependencies": {
    "axios": "0.15.3",
    "classnames": "2.2.5",
    "element-class": "github:valentinvoilean/element-class",
    "immutable": "^4.0.0-rc.1",
    "lodash": "4.17.4",
    "modernizr": "3.3.1",
    "react": "15.4.2",
    "react-addons-css-transition-group": "15.4.2",
    "react-addons-transition-group": "^15.4.2",
    "react-burger-menu": "^1.10.14",
    "react-copy-to-clipboard": "4.2.3",
    "react-dom": "15.4.2",
    "react-match-media": "2.0.1",
    "react-redux": "^5.0.3",
    "react-tabs": "0.8.2",
    "redux": "3.6.0",
    "redux-immutable-state-invariant": "^2.0.0",
    "redux-thunk": "2.2.0",
    "sortablejs": "github:valentinvoilean/Sortable",
    "toggle-selection": "1.0.5",
    "uuid": "3.0.1"
  }
}
